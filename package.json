{
  "name": "wayfarer-node-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/src/app.js --watch",
    "dev": "nodemon src/app.ts",
    "test": "jest --verbose ",
    "lint": "eslint src/app.ts",
    "format": "eslint src/**/*.ts --fix",
    "migrate": "node_modules/.bin/node-pg-migrate -j sql --migrations-dir src/migrations -d DB_URL",
    "migrate:ci:test": "node_modules/.bin/node-pg-migrate -j sql --migrations-dir src/migrations -d TEST_DB_URL"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.2",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "express-validator": "7.0.1",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.0",
    "node-pg-migrate": "6.2.2",
    "pg": "8.11.3",
    "rimraf": "5.0.5",
    "supertest": "6.3.4",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@babel/preset-env": "7.24.0",
    "@babel/preset-typescript": "7.23.3",
